{
  "name": "zadanie/symfony-app",
  "type": "project",
  "require": {
    "php": "^8.1",
    "doctrine/annotations": "^2.0",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.6",
    "doctrine/orm": "^2.16",
    "symfony/asset": "^6.4",
    "symfony/dotenv": "^6.4",
    "symfony/form": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-client": "^6.4",
    "symfony/monolog-bundle": "^3.9",
    "symfony/runtime": "^6.4",
    "symfony/security-bundle": "^6.4",
    "symfony/security-csrf": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/skeleton": "6.4.*",
    "symfony/twig-bundle": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/yaml": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/browser-kit": "6.4.*",
    "symfony/css-selector": "6.4.*",
    "symfony/maker-bundle": "^1.53",
    "symfony/phpunit-bridge": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "App\\\\": "src/"
    },
    "classmap": [
      "src/",
      "migrations/"
    ]
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "6.4.*"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
